// 8. Pick a Number, Any Number

@fn add(x: Nat, y: Nat): Nat = nat_rec(y, x, (_prev, almost) => add1(almost))

// NOTE increase
@fn incr(x: Nat): Nat = nat_rec(x, 1, (_prev, almost) => add(1, almost))

@show add(1)(3)
@show incr(3)

// NOTE `add(1)` is not equal to `incr` by reflection, we need to prove it.
