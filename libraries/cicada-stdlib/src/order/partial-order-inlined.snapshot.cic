import "order/partial-order.cic" { PartialOrder }
import "order/pre-order.cic" { PreOrder }

class PartialOrderInlined [
  Elem: Type
  Under(Elem, Elem): Type

  reflexive(x: Elem): Under(x, x)
  transitive(
    x y z: Elem,
    Under(x, y),
    Under(y, z)
  ): Under(x, z)

  antisymmetric(
    x y: Elem,
    Under(x, y),
    Under(y, x),
  ): Equal(Elem, x, y)
]


the Equal(Type, PartialOrder, PartialOrderInlined)
same
