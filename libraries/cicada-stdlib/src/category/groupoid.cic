@import "category/category.cic" { Category }
@import "category/isomorphism.cic" { Isomorphism }

@class Groupoid @extends Category [
  inv(x y: Object, f: Morphism(x, y)): Morphism(y, x)

  // NOTE The following use of `Isomorphism`
  //   is an example of "partly fulfilled object construction".

  inv_iso(x y: Object, f: Morphism(x, y)): Isomorphism({
    Object
    Morphism
    id
    compose
    id_left
    id_right
    compose_associative
  }, x, y, f, inv(x, y, f))
]
