@import "category/category.cic" { Category }
@import "category/isomorphism.cic" { Isomorphism }

// NOTE This class is an example of "named super class".
@class Groupoid @extends (cat: Category) [
  inv(x y: Object, f: Morphism(x, y)): Morphism(y, x)

  // NOTE The following use of `Isomorphism(cat, f, inv(x, y)(f))`
  //   is an example of "partly fulfilled object construction".
  inv_iso(x y: Object, f: Morphism(x, y)): Isomorphism(cat, x, y, f, inv(x, y, f))
]

// @show Groupoid
