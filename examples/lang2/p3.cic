{
  one = succ(zero)
  two = succ(one)
  three = succ(two)
  add = (x, y) => Nat.ind(y, (_) => Nat, x, (prev, almost) => succ(almost))
      : (Nat) -> (Nat) -> Nat
  mul = (x, y) => Nat.ind(y, (_) => Nat, zero, (prev, almost) => add(almost, x))
      : (Nat) -> (Nat) -> Nat
  pow = (x, y) => Nat.ind(y, (_) => Nat, one, (prev, almost) => mul(almost, x))
      : (Nat) -> (Nat) -> Nat
  eight = {
    four = add(two, two)
    add(four, four)
  }
  _ = same : Equal(Nat, eight, pow(two, three))
  _
}
