@claim swap [ 'A 'B ] [ 'B 'A ]
@define swap [ (b) (a) b a ]

@claim dup [ 'A ] [ 'A 'A ]
@define dup [ (a) a a ]

@claim drop [ 'A ] [ ]
@define drop [ (a) ]

@claim apply [ 'A @arrow [ 'A ] [ 'B ] ] [ 'B ]
@define apply [ (f) f ]

@show [ "a" "b" [ dup ] ]
@show [ "a" [ drop ] apply ]
@show [ "a" [ dup dup ] apply ]
@show [ "a" "b" [ (_) dup ] apply ]

@show [ 'a 'b [ dup ] ]
@show [ 1 2 [ dup ] apply ]

@show [ 3.14 dup ]
