@def add = @the (Nat) -> (Nat) -> Nat
  (x) => (y) => nat_ind(y, (_) => Nat, x, (prev) => (almost) => add1(almost))


@def mul = @the (Nat) -> (Nat) -> Nat
  (x) => (y) => nat_ind(y, (_) => Nat, 0, (prev) => (almost) => add(almost)(x))


@def pow = @the (Nat) -> (Nat) -> Nat
  (x) => (y) => nat_ind(y, (_) => Nat, 1, (prev) => (almost) => mul(almost)(x))

@def eight =
  @let four = add(2)(2)
  add(four)(four)

@show @the Equal(Nat, eight, pow(2)(3)) same
