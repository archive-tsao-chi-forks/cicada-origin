// 2. Doin' What Comes Naturally

{
  flavored_lentils = (flavor) => cons(flavor, "lentils"): (String) -> Pair(String, String)
  garlic_flavored_lentils = flavored_lentils("garlic"): Pair(String, String)
  id = (x) => x : (String) -> String
  T = (x2: String) * (x3: String) -> String : Type
  id_in_pair = (x) => cons(x, (x) => x) : (x: String) -> T
  _ = id_in_pair("x"): T
  _ = same: Equal(T, id_in_pair("x"), cons("x", (y) => y))
  _
}
