@datatype Nat : Type {
  zero: Nat
  succ: (prev: Nat) -> Nat
}

@show Nat
@show Nat.zero
@show Nat.succ
@show Nat.succ(Nat.zero)

@datatype Bool : Type {
  true: Bool
  false: Bool
}

@show Bool
@show Bool.true
@show Bool.false

true : Bool
true = Bool.true

zero_p : (Nat) -> Bool
zero_p = (Nat.zero) => Bool.true

@show zero_p(Nat.zero)
