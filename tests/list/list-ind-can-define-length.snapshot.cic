@def step_length: (E: Type) -> (e: E, es: List(E), almost: Nat) -> Nat =
  (E) => (e, es, almost) => add1(almost)

@def length: (E: Type) -> (List(E)) -> Nat =
  (E) => (x) => list_ind(x, (_) => Nat, 0, step_length(E))

@def expectations: List(String) = @li [
  "cooked",
  "eaten",
  "tried-cleaning",
  "understood",
  "slept",
]

@show
length(String, expectations)
