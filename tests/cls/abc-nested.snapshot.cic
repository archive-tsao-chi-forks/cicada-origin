@def the_nat_type
@the (Trivial) -> Type
(_) => Nat

@class ABC [ a: Trivial, b: the_nat_type(a), c: String ]

@def abc @the ABC
{ a: sole, b: 2, c: "c" }

@def xyz
@the [
  x: String,
  y: String,
  z: ABC
] {
  x: "x",
  y: "y",
  z: abc,
}

@show xyz

@show xyz.z
@show xyz.z.a
@show xyz.z.b
@show xyz.z.c
