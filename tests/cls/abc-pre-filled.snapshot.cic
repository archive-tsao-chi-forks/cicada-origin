@def the_nat_type
@the (Trivial) -> Type
(_) => Nat

@class ABC [ a: Trivial = sole, b: the_nat_type(a), c: String ]

@def abc @the ABC(2, "c")
{ a: sole, b: 2, c: "c" }

@show abc

@show abc.a
@show abc.b
@show abc.c


@class ABCDE [ a: Trivial, b: the_nat_type(a), c: String = "c", d: String, e: String ]

@def abcde @the ABCDE(sole, 2, "d")
{ a: sole, b: 2, c: "c", d: "d", e: "e" }

@show abcde

@show abcde.a
@show abcde.b
@show abcde.c
@show abcde.d
@show abcde.e
