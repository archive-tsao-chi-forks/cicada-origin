class unit_t {}
object unit {}

class list_t { given A : type }
class null_t { given A : type }
class cons_t {
  given A : type
  given car : A
  given cdr : list_t(A)
}

define null_unit : null_t(unit_t) = { let A = unit_t }

let cons_unit_list = {
  given unit_list : list_t(unit_t)
  return {
    let A = unit_t
    let car = unit
    let cdr = unit_list
  }
}

@show cons_unit_list(cons_unit_list(null_unit))
