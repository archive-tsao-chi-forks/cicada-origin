@module prelude.structure.category.Category

@class Epimorphism {
  cat: Category

  @given x: cat.Object, y: cat.Object
  morphism(): cat.Morphism(x, y)

  @given y: cat.Object, z: cat.Object
  cancel_left(
    f: cat.Morphism(y, z),
    g: cat.Morphism(y, z),
    Equal(
      cat.compose(morphism, f),
      cat.compose(morphism, g)),
  ): Equal(f, g)
}
