package prelude.structure.category.Category

class Isomorphism {
  cat: Category

  @given(a: cat.Object, b: cat.Object)
  morphism(): cat.Morphism(a, b)

  @given(a: cat.Object, b: cat.Object)
  inverse(): cat.Morphism(b, a)

  inverse_left: Equal(cat.compose(morphism, inverse), cat.id(a))

  inverse_right: Equal(cat.compose(inverse, morphism), cat.id(b))
}
