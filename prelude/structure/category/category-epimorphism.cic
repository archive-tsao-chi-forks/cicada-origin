package prelude.structure.category.Category

class Epimorphism {
  cat: Category

  @given(a: cat.Object, b: cat.Object)
  morphism(): cat.Morphism(a, b)

  @given(b: cat.Object, c: cat.Object)
  cancel_left(
    f: cat.Morphism(b, c),
    g: cat.Morphism(b, c),
    Equal(
      cat.compose(morphism, f),
      cat.compose(morphism, g)),
  ): Equal(f, g)
}
