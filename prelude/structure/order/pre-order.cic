package prelude.structure.order.PreOrder

// NOTE
// - thin category
// - hom_set(A, B).size <= 1
class PreOrder {
  Elem: Type
  Under(Elem, Elem): Type

  reflexive(a: Elem): Under(a, a)

  @given(a: Elem, b: Elem, c: Elem)
  transitive(Under(a, b), Under(b, c)): Under(a, c)
}

class StrictUnder {
  order: PreOrder
  x: order.Elem
  y: order.Elem
  strict: NonEqual(x, y)
  under: order.Under(x, y)
}
