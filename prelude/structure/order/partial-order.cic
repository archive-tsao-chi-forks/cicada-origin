class PartialOrder extends PreOrder {
  antisymmetric : {
    Pre(a, b)
    Pre(b, a)
    ---------
    Equation(a, b)
  }
}

develop PartialOrder {
  cover : { a, b : Elem -> type } = {
    StrictPre(a, b) &
      { x : Elem
        Pre(a, x)
        StrictPre(x, b)
        ---------
        Equation(Elem, x, a) }
  }
}

