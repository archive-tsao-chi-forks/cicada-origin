class Monomorphism {
  cat: Category

  @given(a: cat.Object, b: cat.Object)
  morphism(): cat.Morphism(a, b)

  @given(a: cat.Object, c: cat.Object)
  cancel_right(
    f: cat.Morphism(c, a),
    g: cat.Morphism(c, a),
    Equal(cat.compose(f, morphism), cat.compose(g, morphism))
  ): Equal(f, g)
}

// NOTE Example:
//   mono: Monomorphism(cat)
//   mono.morphism(a, b): cat.Morphism(a, b)
