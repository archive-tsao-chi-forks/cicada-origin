class Monomorphism {
  cat: Category
  morphism(# a b: cat.Object): cat.Morphism(a, b)

  cancel_right(
    # c: cat.Object,
    f g: cat.Morphism(c, a),
    Equal(cat.compose(f, morphism), cat.compose(g, morphism))
  ): Equal(f, g)
}

// NOTE Example:
//   mono: Monomorphism(cat)
//   mono.morphism(a, b): cat.Morphism(a, b)
