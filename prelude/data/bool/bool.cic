package prelude.data.bool.Bool

data Bool {
  True: Bool
  False: Bool
}

not(x: Bool): Bool
not(True) = False
not(False) = True

and(x: Bool, y: Bool): Bool
and(True, True) = True
and(True, False) = False
and(False, True) = False
and(False, False) = False

or(x: Bool, y: Bool): Bool
or(True, True) = True
or(True, False) = True
or(False, True) = True
or(False, False) = False
